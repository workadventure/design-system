
.possibility {
    @apply w-40 mr-8 last:mr-0 px-8 aspect-square bg-white border border-solid border-neutral-400 rounded-xl shadow-lg text-center;
    .possibility-icon {
        @apply -mt-4;
    }
    .possibility-content {
        @apply flex h-16;
        .possibility-text {
            @apply paragraph-lg m-auto;
        }
    }
}
